<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Aurum.Views"
        Width="200" Height="300"
        x:Class="Aurum.Views.MainWindow"
        Icon="/Assets/Golden_Crystal_Borderless-64x64.ico"
        Title="Aurum"
        mc:Ignorable="d"
        xmlns:rootViewModel="clr-namespace:Aurum.ViewModels;assembly=Aurum"
        d:DataContext="{d:DesignInstance rootViewModel:MainWindowViewModel}">
        <views:MainControlView DataContext="{Binding List}"/>
</Window>
